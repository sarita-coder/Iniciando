							hr.ln1
							p 
								a.actitudes(href="") Naturaleza
						
						.contenedor.etiqueta.etiq2
				.col-sm-9.pd-50
					.row
						.col-foto
							p Sarita Román Colonio  
							hr.ln1
							p 
								a.carrera(href="") ECONOMISTA
								a.carrera(href="") FRONTED
								a.carrera(href="") GEOMÁTICA
